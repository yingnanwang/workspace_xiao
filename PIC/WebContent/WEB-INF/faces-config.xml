<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>imagePath</managed-bean-name>
		<managed-bean-class>global.ImagePath</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
		<property-name>path1</property-name>
		<value>images/img1.jpg</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchPageImages</managed-bean-name>
		<managed-bean-class>global.SearchPageImages</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
